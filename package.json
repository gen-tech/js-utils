{
  "name": "@gen-tech/js-utils",
  "version": "0.8.4",
  "description": "Javascript utility functions for web development",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://gen-tech.github.io/js-utils",
  "keywords": [
    "array",
    "compact",
    "promise",
    "defer",
    "is truthy",
    "is falsy",
    "math",
    "min max",
    "noop",
    "unique id"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gen-tech/js-utils.git"
  },
  "scripts": {
    "prepare": "tsc",
    "document": "typedoc --out docs --excludeNotExported --disableOutputCheck --gitRevision master",
    "test": "NODE_ENV=test nyc mocha --recursive --compilers ts:ts-node/register src/*.spec.ts src/**/*.spec.ts",
    "cover": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "alisahinozcelik@gmail.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "ts-node": "^6.0.3",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "./src/index.ts",
      "./src/**/*.spec.ts",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
