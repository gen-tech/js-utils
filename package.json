{
  "name": "@gen-tech/js-utils",
  "version": "0.0.0",
  "description": "Javascript utility functions for web development",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://gen-tech.github.io/js-utils",
  "keywords": [
    "object",
    "deep clone",
    "array",
    "compact",
    "difference",
    "intersecion",
    "promise",
    "defer",
    "open",
    "is truthy",
    "is falsy",
    "math",
    "min max",
    "noop",
    "unique id"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gen-tech/js-utils.git"
  },
  "scripts": {
    "prepublishOnly": "gtvu $TRAVIS_TAG && tsc",
    "document": "typedoc --out docs --excludeNotExported --excludePrivate --disableOutputCheck --gitRevision master --exclude \"**/+(as-proto|as-static)/**/*ts\"",
    "prepare-github-pages": "npm run document && echo \"\" > ./docs/.nojekyll",
    "test": "nyc mocha --recursive --compilers ts:ts-node/register src/*.spec.ts src/**/*.spec.ts",
    "cover": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "alisahinozcelik@gmail.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@gen-tech/version-update": "^0.1.2",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.3",
    "update-json": "^1.0.0"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "./src/index.ts",
      "./src/**/*.spec.ts",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
