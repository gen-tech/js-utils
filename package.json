{
  "name": "@thalesrc/js-utils",
  "version": "0.0.0",
  "description": "Javascript utility functions for web development",
  "main": "index.js",
  "typings": "index.d.ts",
  "homepage": "https://thalesrc.github.io/js-utils",
  "keywords": [
    "object",
    "deep clone",
    "array",
    "compact",
    "difference",
    "intersecion",
    "promise",
    "defer",
    "open",
    "is truthy",
    "is falsy",
    "math",
    "min max",
    "noop",
    "unique id"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thalesrc/js-utils.git"
  },
  "scripts": {
    "document": "typedoc --out docs --excludeNotExported --excludePrivate --disableOutputCheck --gitRevision master --exclude \"**/+(as-proto|as-static)/**/*ts\"",
    "prepare-github-pages": "npm run document && echo \"\" > ./docs/.nojekyll",
    "test": "nyc mocha --recursive --compilers ts:ts-node/register src/*.spec.ts src/**/*.spec.ts",
    "cover": "nyc report --reporter=text-lcov | codecov",
    "publish:config-npm": "replace '<EMAIL>' $NPM_EMAIL .npmrc && replace '<TOKEN>' $NPM_TOKEN .npmrc",
    "publish:copy-files": "cpy 'LICENSE' 'README.md' '.npmrc' '.npmignore' 'package.json' dist",
    "publish:pack": "cd dist && npm pack",
    "publish:prepare": "gtvu $TRAVIS_TAG && tsc && npm run publish:config-npm && npm run publish:copy-files && npm run publish:pack",
    "publish:publish": "npm publish ./dist/thalesrc-js-utils-$TRAVIS_TAG.tgz",
    "prepare-and-publish": "npm run publish:prepare && npm run publish:publish"
  },
  "author": "alisahinozcelik@gmail.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@gen-tech/version-update": "^0.1.2",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "codecov.io": "^0.1.6",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "replace": "^1.0.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.3",
    "update-json": "^1.0.0"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "./src/index.ts",
      "./src/**/*.spec.ts",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
